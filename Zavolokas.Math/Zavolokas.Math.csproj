<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0</TargetFrameworks>
		<AssemblyName>Zavolokas.Math</AssemblyName>
		<RootNamespace>Zavolokas.Math</RootNamespace>
		<Title>Zavolokas.Math</Title>
        <Version>1.0.1.0</Version>
        <Copyright>Copyright 2019 Sergey Zavoloka</Copyright>
        <Description>Contains useful bits to tackle different math problems.</Description>
        <Authors>Sergey Zavoloka</Authors>
        <Company>Sergey Zavoloka</Company>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageProjectUrl>https://github.com/zavolokas/Math</PackageProjectUrl>
        <RepositoryUrl>https://github.com/zavolokas/Math</RepositoryUrl>
    </PropertyGroup>
	
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<GenerateSerializationAssemblies>Auto</GenerateSerializationAssemblies>
	</PropertyGroup>
	
</Project>